<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="pixedit" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Linux Debug">
				<Option platforms="Unix;Mac;" />
				<Option output="pixedit" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj\Debug\" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-g" />
					<Add option="`sdl-config --cflags`" />
				</Compiler>
				<Linker>
					<Add option="`sdl-config --libs`" />
				</Linker>
			</Target>
			<Target title="Linux Release">
				<Option platforms="Unix;Mac;" />
				<Option output="pixedit" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj\Release\" />
				<Option type="0" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-O2" />
					<Add option="`sdl-config --cflags`" />
				</Compiler>
				<Linker>
					<Add option="-s" />
					<Add option="`sdl-config --libs`" />
				</Linker>
			</Target>
			<Target title="Windows Debug">
				<Option platforms="Windows;" />
				<Option output="pixedit" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj\Debug\" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-g" />
				</Compiler>
				<Linker>
					<Add option="-mwindows" />
					<Add library="mingw32" />
					<Add library="SDLmain" />
					<Add library="SDL" />
				</Linker>
			</Target>
			<Target title="Windows Release">
				<Option platforms="Windows;" />
				<Option output="pixedit" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj\Release\" />
				<Option type="0" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-O2" />
				</Compiler>
				<Linker>
					<Add option="-s" />
					<Add option="-mwindows" />
					<Add library="mingw32" />
					<Add library="SDLmain" />
					<Add library="SDL" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wall" />
			<Add option="-fexceptions" />
		</Compiler>
		<Unit filename="pixedit.c">
			<Option compilerVar="CC" />
		</Unit>
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
			<lib_finder disable_auto="1" />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
